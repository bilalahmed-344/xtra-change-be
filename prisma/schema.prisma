
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User {
  id      String   @id @default(uuid())
  code        String   
  phoneNumber String    @unique
  name        String?
  email       String?   @unique
  dob         String?   
  address     String?   
  gender     String?
  idCardFrontSide String?    
  idCardBackSide String?    
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}


